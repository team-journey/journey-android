<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mohaeng_yellow9"
    tools:context=".setting.view.TeamFragment">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_team_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imagebutton_team_back"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/ic_diary_back_page_button"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.appcompat.widget.Toolbar>

    <ImageView
        android:id="@+id/imageview_team_back"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="18dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_team_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_team_title" />

    <TextView
        android:id="@+id/textview_team_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="63dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="63dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:text="모행을 만든 사람들"
        android:textColor="@color/mohaeng_yellow_c"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toTopOf="@+id/imageview_team_back" />

    <TextView
        android:id="@+id/textview_team_design"
        android:layout_width="110dp"
        android:layout_height="28dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/team_round_text"
        android:fontFamily="@font/spoqahansansneoregular"
        android:gravity="center"
        android:text="Design"
        android:textColor="@color/mohaeng_yellow11"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_title" />

    <TextView
        android:id="@+id/textview_team_dname1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="박시원"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_dname2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_design" />

    <TextView
        android:id="@+id/textview_team_dname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="최유리"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_dname3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textview_team_dname1"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_design" />

    <TextView
        android:id="@+id/textview_team_dname3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="조아라"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textview_team_dname2"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_design" />

    <TextView
        android:id="@+id/textview_team_ios"
        android:layout_width="110dp"
        android:layout_height="28dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/team_round_text"
        android:fontFamily="@font/spoqahansansneoregular"
        android:gravity="center"
        android:text="iOS"
        android:textColor="@color/mohaeng_yellow11"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_dname2" />

    <TextView
        android:id="@+id/textview_team_iname1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="정초이"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_iname2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_ios" />

    <TextView
        android:id="@+id/textview_team_iname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="윤예지"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_iname3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textview_team_iname1"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_ios" />

    <TextView
        android:id="@+id/textview_team_iname3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="김승찬"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_iname4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textview_team_iname2"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_ios" />

    <TextView
        android:id="@+id/textview_team_iname4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="김윤서"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintStart_toEndOf="@+id/textview_team_iname3"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_ios" />


    <TextView
        android:id="@+id/textview_team_aos"
        android:layout_width="110dp"
        android:layout_height="28dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/team_round_text"
        android:fontFamily="@font/spoqahansansneoregular"
        android:gravity="center"
        android:text="AOS"
        android:textColor="@color/mohaeng_yellow11"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_iname2" />

    <TextView
        android:id="@+id/textview_team_aname1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="박주예"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_aname2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_aos" />

    <TextView
        android:id="@+id/textview_team_aname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="김기현"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textview_team_aname1"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_aos" />

    <TextView
        android:id="@+id/textview_team_server"
        android:layout_width="110dp"
        android:layout_height="28dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/team_round_text"
        android:fontFamily="@font/spoqahansansneoregular"
        android:gravity="center"
        android:text="AOS"
        android:textColor="@color/mohaeng_yellow11"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_aname2" />

    <TextView
        android:id="@+id/textview_team_sname1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="김현지"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textview_team_sname2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/imageview_team_back"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_server" />

    <TextView
        android:id="@+id/textview_team_sname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/gmarketsansttfmedium"
        android:gravity="center"
        android:text="김기연"
        android:textColor="@color/mohaeng_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_team_back"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textview_team_sname1"
        app:layout_constraintTop_toBottomOf="@+id/textview_team_server" />

    <ImageView
        android:id="@+id/imageview_team"
        android:layout_width="144dp"
        android:layout_height="156dp"
        android:src="@drawable/ic_team_img"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>