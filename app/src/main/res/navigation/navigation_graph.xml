<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/frameFragment">

    <fragment
        android:id="@+id/frameFragment"
        android:name="org.journey.android.frame.FrameFragment"
        android:label="fragment_frame"
        tools:layout="@layout/fragment_frame" >
        <action
            android:id="@+id/action_frameFragment_to_splashFragment"
            app:destination="@id/splashFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="org.journey.android.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="org.journey.android.login.view.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="org.journey.android.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_communityFragment"
            app:destination="@id/communityFragment" />
        <action
            android:id="@+id/action_mainFragment_to_challengeFragment2"
            app:destination="@id/challengeFragment" />
        <action
            android:id="@+id/action_mainFragment_to_rewardFragment"
            app:destination="@id/rewardFragment" />
    </fragment>
    <fragment
        android:id="@+id/communityFragment"
        android:name="org.journey.android.community.CommunityFragment"
        android:label="fragment_community"
        tools:layout="@layout/fragment_community" >
        <action
            android:id="@+id/action_communityFragment_to_diaryFirstFragment"
            app:destination="@id/diaryFirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeFragment"
        android:name="org.journey.android.challenge.ChallengeFragment"
        android:label="ChallengeFragment"
        tools:layout="@layout/fragment_challenge">
        <action
            android:id="@+id/action_challengeFragment_to_courseFragment"
            app:destination="@id/courseFragment" />
    </fragment>
    <fragment
        android:id="@+id/diaryFirstFragment"
        android:name="org.journey.android.diary.DiaryFirstFragment"
        android:label="DiaryFirstFragment"
        tools:layout="@layout/fragment_diary_first">
        <action
            android:id="@+id/action_diaryFirstFragment_to_diarySecondFragment"
            app:destination="@id/diarySecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/diarySecondFragment"
        android:name="org.journey.android.diary.DiarySecondFragment"
        android:label="DiarySecondFragment"
        tools:layout="@layout/fragment_diary_second">
        <action
            android:id="@+id/action_diarySecondFragment_to_privateDetailFragment"
            app:destination="@id/privateDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/rewardFragment"
        android:name="org.journey.android.reward.RewardFragment"
        android:label="RewardFragment"
        tools:layout="@layout/fragment_reward" />
    <fragment
        android:id="@+id/privateDetailFragment"
        android:name="org.journey.android.diary.PrivateDetailFragment"
        android:label="PrivateDetailFragment"
        tools:layout="@layout/fragment_private_detail" />
    <fragment
        android:id="@+id/courseFragment"
        android:name="org.journey.android.course.CourseFragment"
        android:label="fragment_course"
        tools:layout="@layout/fragment_course">
        <action
            android:id="@+id/action_courseFragment_to_libraryFragment"
            app:destination="@id/libraryFragment" />
    </fragment>
    <fragment
        android:id="@+id/libraryFragment"
        android:name="org.journey.android.course.LibraryFragment"
        android:label="fragment_library"
        tools:layout="@layout/fragment_library" />
</navigation>